# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Main
GET        /*path                            controllers.Application.call(path: String)
PUT        /*path                            controllers.Application.call(path: String)
DELETE     /*path                            controllers.Application.call(path: String)
POST       /*path                            controllers.Application.call(path: String)


# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file                                                                           controllers.Assets.at(path="/public", file)
